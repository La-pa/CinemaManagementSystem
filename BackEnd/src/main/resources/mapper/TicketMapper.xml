<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.mapper.TicketMapper">

    <resultMap id="BaseResultMap" type="com.example.backend.entity.Ticket">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="sessionId" column="session_id" jdbcType="INTEGER"/>
            <result property="seatId" column="seat_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,session_id,seat_id,
        price
    </sql>
</mapper>

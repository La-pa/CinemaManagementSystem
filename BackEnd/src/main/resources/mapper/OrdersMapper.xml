<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.mapper.OrdersMapper">

    <resultMap id="BaseResultMap" type="com.example.backend.entity.Orders">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="purchaseTime" column="purchase_time" jdbcType="TIMESTAMP"/>
            <result property="ticketId" column="ticket_id" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,purchase_time,ticket_id,
        user_id
    </sql>
</mapper>

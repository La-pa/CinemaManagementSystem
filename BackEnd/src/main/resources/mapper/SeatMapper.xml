<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.mapper.SeatMapper">

    <resultMap id="BaseResultMap" type="com.example.backend.entity.Seat">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="seatRow" column="seat_row" jdbcType="CHAR"/>
            <result property="seatCol" column="seat_col" jdbcType="INTEGER"/>
            <result property="roomId" column="room_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,seat_row,seat_col,
        room_id
    </sql>
</mapper>
